<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="3fe450f2-ad26-44f6-81e0-683a3c8920dd" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Push succesfull nightly build to S3</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_537" name="Push to nightly S3" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[module load aws
aws s3 cp ribasim_cli.zip s3://ribasim/teamcity/Ribasim_Ribasim/BuildRibasimCliWindows/latest/ribasim_cli.zip]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements>
      <does-not-equal id="RQ_277" name="env.OS" value="Windows_NT" />
    </requirements>
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_157" sourceBuildTypeId="BuildRibasimCliWindows" cleanDestination="true">
        <revisionRule name="lastSuccessful" revision="latest.lastSuccessful" />
        <artifact sourcePath="ribasim_cli.zip" />
      </dependency>
    </artifact-dependencies>
    <cleanup />
  </settings>
</build-type>

